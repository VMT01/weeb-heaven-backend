{
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ],
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "printWidth": 120,
    "tabWidth": 4,
    "arrowParens": "avoid",
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^@nest/(.*)$",
        "^@configs/(.*)$",
        "^@constants/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true,
    "importOrderParserPlugins": [
        "classProperties",
        "decorators-legacy",
        "typescript"
    ]
}
